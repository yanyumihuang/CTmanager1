<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.edu.huel.ctmanager.dao.TitleDao">

<select id="selectT_id" parameterType="String" resultType="int">
select t_id from staff_title where t_name=#{t_name}
</select>

<select id="select_T_name" resultType="String">
select distinct t_name from staff_title
</select>
</mapper>